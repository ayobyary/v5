{
  "name": "v5",
  "version": "1.0.0",
  "description": "Full Stack Angular + Node.js Project",
  "scripts": {
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && npm run dev",
    "backend:prod": "cd backend && npm start",
    "frontend": "cd angular-project && ng serve",
    "frontend:build": "cd angular-project && ng build",
    "frontend:build:prod": "cd angular-project && ng build --configuration production",
    "install-all": "npm install && cd backend && npm install && cd ../angular-project && npm install",
    "build": "npm run frontend:build",
    "build:prod": "npm run frontend:build:prod"
  },
  "keywords": [
    "angular",
    "nodejs",
    "express",
    "fullstack",
    "jwt",
    "authentication"
  ],
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
